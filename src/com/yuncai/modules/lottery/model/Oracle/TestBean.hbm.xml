<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.yuncai.modules.lottery.model.Oracle.testBean" table="username">
        <id name="id" type="java.lang.Integer">
            <column name="id" not-null="true"/>
        </id>
        <property name="name" type="java.lang.String">
            <column name="name" length="40" not-null="true" />
        </property>
        <property name="pwd" type="java.lang.String">
            <column name="pwd" length="50" not-null="true"/>
        </property>
        
        <property name="email" type="java.lang.String">
            <column name="email" length="40" />
        </property>
        <property name="moble" type="java.lang.String">
            <column name="moble" length="40" not-null="true" />
        </property>
        
        <property name="demo" type="java.lang.String">
            <column name="demo" length="50" />
        </property>
    </class>
</hibernate-mapping>
